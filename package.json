{
  "name": "ai-pipeline-orchestrator",
  "version": "0.1.0",
  "description": "Production-ready orchestration framework for AI applications featuring hybrid intent classification, dynamic context optimization, and sequential pipeline architecture",
  "keywords": [
    "ai",
    "orchestration",
    "intent-classification",
    "context-optimization",
    "chatbot",
    "llm",
    "claude",
    "anthropic"
  ],
  "author": "Emmanuel Adu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emmanuel-adu/ai-pipeline-orchestrator"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./intent": {
      "types": "./dist/intent/index.d.ts",
      "import": "./dist/intent/index.mjs",
      "require": "./dist/intent/index.js"
    },
    "./context": {
      "types": "./dist/context/index.d.ts",
      "import": "./dist/context/index.mjs",
      "require": "./dist/context/index.js"
    },
    "./handlers": {
      "types": "./dist/handlers/index.d.ts",
      "import": "./dist/handlers/index.mjs",
      "require": "./dist/handlers/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.mjs",
      "require": "./dist/providers/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build",
    "example:chat": "tsx examples/chat-cli.ts",
    "example:basic": "tsx examples/basic-chatbot.ts",
    "example:complete": "tsx examples/complete-chatbot.ts",
    "example:streaming": "tsx examples/streaming-chatbot.ts",
    "example:all": "tsx examples/all-handlers.ts"
  },
  "dependencies": {
    "zod": "^3.25.0"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": ">=0.0.51",
    "@ai-sdk/openai": ">=0.0.59",
    "ai": ">=4.0.0",
    "ollama-ai-provider": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    },
    "ai": {
      "optional": true
    },
    "ollama-ai-provider": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^3.0.9",
    "@ai-sdk/openai": "^1.2.2",
    "@types/node": "^20.0.0",
    "ai": "^6.0.23",
    "dotenv": "^17.2.3",
    "ollama-ai-provider": "^1.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
